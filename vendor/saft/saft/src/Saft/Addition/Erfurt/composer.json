    "authors": [
        {
            "name": "Natanael Arndt",
            "homepage": "http://aksw.org/NatanaelArndt",
            "email": "arndtn@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Konrad Abicht",
            "homepage": "http://www.inspirito.de",
            "email": "hi@inspirito.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",

        "saft/saft-rdf": "dev-master",
        "saft/saft-store": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*"
    },
    "autoload": {
        "psr-4": {
            "Saft\\Addition\\Erfurt\\": "."
        }
    },
    "minimum-stability":"dev"
